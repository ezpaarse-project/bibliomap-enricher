  "description": "Script used to parse in ezPAARSE log.io harvested log lines in real time.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "insertversion": "sed -i \"s#\\(image: ezpaarseproject/ezpaarse2log.io:\\)\\([\\.a-z0-9]\\+\\)#\\1${npm_package_version}#g\" docker-compose.yml",
    "version": "npm run insertversion && git add docker-compose.yml",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ezpaarse-project/ezpaarse2log.io.git"
  },
