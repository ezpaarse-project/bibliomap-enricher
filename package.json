  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ezpaarse-project/ezpaarse2log.io/issues"
  },
  "homepage": "https://github.com/ezpaarse-project/ezpaarse2log.io",
  "dependencies": {
    "JSONStream": "^0.8.4",
    "debug": "^2.6.0",
    "event-stream": "^3.1.7",
    "log.io-server-parser": "^0.1.1",
    "nconf": "^0.6.9",
    "request": "^2.39.0"
  }
}
